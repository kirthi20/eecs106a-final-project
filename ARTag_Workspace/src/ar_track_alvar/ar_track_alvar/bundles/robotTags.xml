<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<multimarker markers="8">
    <!-- point labeling convention: start from bottom left, and go counter clockwise -->
    <!-- master AR Tag, located above blue batteries -->
    <marker index="0" status="1">
        <!-- centered @ 0, 0, 0-->
        <corner x="-2" y="-2" z="0" />
        <corner x="2" y="-2" z="0" />
        <corner x="2" y="2" z="0" />
        <corner x="-2" y="2" z="0" />
    </marker>

    <!-- located below camera-->
    <marker index="1" status="1">
         <!-- centered @ 0, -5,-16 -->
        <corner x="2" y="-7" z="-16" />
        <corner x="-2" y="-7" z="-16" />
        <corner x="-2" y="-3" z="-16" />
        <corner x="2" y="-3" z="-16" />
    </marker>

    <!-- located on the side of the covered up AR tag -->
    <marker index="2" status="1">
         <!-- centered @ 12, 0, -8 -->
        <corner x="12" y="-2" z="-10" />
        <corner x="12" y="-2" z="-6" />
        <corner x="12" y="2" z="-6" />
        <corner x="12" y="2" z="-10" />
    </marker>


    <!-- located on the side of mango label -->
    <marker index="3" status="1">
        <!-- centered @ -8, 0,-8 -->
        <corner x="-8" y="-2" z="-6" />
        <corner x="-8" y="-2" z="-10" />
        <corner x="-8" y="2" z="-10" />
        <corner x="-8" y="2" z="-6" />
    </marker>

    <!-- located on the corner of the camera and mango label-->
    <marker index="12" status="1">
        <!-- centered @ _ _ _ -->
        <corner x="-5.2" y="-2" z="-14.8" />
        <corner x="-8" y="-2" z="-12" />
        <corner x="-8" y="2" z="-12" />
        <corner x="-5.2" y="2" z="-14.8" />
    </marker>
    
    <!-- located on the corner of the camera and side opposite to mango label-->
    <marker index="13" status="1">
        <!-- centered @ _ _ _ -->
        <corner x="8" y="-2" z="-12" />
        <corner x="5.2" y="-2" z="-14.8" />
        <corner x="5.2" y="2" z="-14.8" />
        <corner x="8" y="2" z="-14.8" />
    </marker>


    <!-- located on the corner of the blue batteries and mango label-->
    <marker index="14" status="1">
        <!-- centered @ _ _ _ -->
        <corner x="-8" y="-2" z="-4" />
        <corner x="-5.2" y="-2" z="-2.2" />
        <corner x="-5.2" y="2" z="-2.2" />
        <corner x="-8" y="2" z="-4" />
    </marker>

    <!-- located on the corner of the blue batteries and side opposite to mango label, a bit down-->
    <marker index="15" status="1">
        <!-- centered @ _ _ _ -->
        <corner x="5.2" y="-6" z="-2.2" />
        <corner x="8" y="-6" z="-4" />
        <corner x="8" y="-2" z="-4" />
        <corner x="5.2" y="-2" z="-2.2" />
    </marker>

</multimarker>
